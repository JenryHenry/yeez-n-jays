<main class="container mt-4">
    <h1 class="mb-3">User Login</h1>
    <form class="yeeznjays-login-form">
        <div class="mb-2">
            <label for="username" class="form-label">User Name</label>
            <input type="text" class="form-control" id="username-yeeznjays-login" placeholder="Enter User Name" autocomplete="username"/>
        </div>
        <div class="mb-2">
            <label for="password" class="form-label">User Password</label>
            <input type="password" class="form-control" id="password-yeeznjays-login" placeholder="Enter User Password" autocomplete="current-password"/>
        </div>
        <button type="submit" class="btn btn-secondary">User Login</button>
    </form>
</main>
<script src="/js/login.js"></script>